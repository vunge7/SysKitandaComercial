	<queryString>
		<![CDATA[SELECT
     tb_saida_vasilhame.`data_saida` AS tb_saida_vasilhame_data_saida,
     tb_saida_vasilhame.`hora_saida` AS tb_saida_vasilhame_hora_saida,
     tb_saida_vasilhame.`pk_saida_vasilhame` AS tb_saida_vasilhame_pk_saida_vasilhame,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_armazem.`codigo` AS tb_armazem_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome
FROM
     `tb_usuario` tb_usuario INNER JOIN `tb_saida_vasilhame` tb_saida_vasilhame ON tb_usuario.`codigo` = tb_saida_vasilhame.`fk_usuario`
     INNER JOIN `tb_armazem` tb_armazem ON tb_saida_vasilhame.`fk_armazem` = tb_armazem.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao

WHERE  tb_armazem.`codigo`  = $P{ID_ARMAZEM}  AND tb_saida_vasilhame.`data_saida` BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}]]>
