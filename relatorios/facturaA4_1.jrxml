     compras.`total_incidencia_isento` AS compras_total_incidencia_isento,
     compras.`data_limite_levantamento` AS compras_data_limite_levantamento,
     compras.`fk_documento` AS compras_fk_documento,
     compras.`fk_fornecedor` AS compras_fk_fornecedor,
     compras.`idArmazemFK` AS compras_idArmazemFK,
     compras.`fornecedor_nif` AS compras_fornecedor_nif,
     compras.`fk_ano_economico` AS compras_fk_ano_economico,
     compras.`autorizado` AS compras_autorizado,
     item_compras.`pk_itm_compras` AS item_compras_pk_itm_compras,
     item_compras.`preco_compra` AS item_compras_preco_compra,
     item_compras.`quantidade` AS item_compras_quantidade,
     item_compras.`valor_iva` AS item_compras_valor_iva,
     item_compras.`motivo_isensao` AS item_compras_motivo_isensao,
