	<queryString>
		<![CDATA[SELECT
     tb_venda.`dataVenda` AS tb_venda_dataVenda,
     tb_usuario.`codigo` AS tb_usuario_codigo,
     SUM(tb_venda.`total_venda`)AS TOTAL_VENDA,
     SUM(tb_venda.`total_geral`)AS TOTAL_VENDA1,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_venda.`codigo` AS tb_venda_codigo,
     tb_venda.`status_eliminado` AS tb_venda_status_eliminado,
     tb_venda.`credito` AS tb_venda_credito
FROM
     `tb_usuario` tb_usuario INNER JOIN `tb_venda` tb_venda ON tb_usuario.`codigo` = tb_venda.`codigo_usuario`
WHERE
    tb_venda.`dataVenda` BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2} AND status_eliminado = 'false' AND credito = 'true']]>
	</queryString>
