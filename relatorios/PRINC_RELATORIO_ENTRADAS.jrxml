	<queryString>
		<![CDATA[SELECT
     tb_usuario.`nome` AS tb_usuario_nome,
     entrada_tesouraria.`pk_entrada_tesouraria` AS entrada_tesouraria_pk_entrada_tesouraria,
     entrada_tesouraria.`descricao` AS entrada_tesouraria_descricao,
     entrada_tesouraria.`valor` AS entrada_tesouraria_valor,
     entrada_tesouraria.`data_entrada` AS entrada_tesouraria_data_entrada,
     entrada_tesouraria.`hora_entrada` AS entrada_tesouraria_hora_entrada,
     tb_dados_instituicao.`idDadosInsitiuicao` AS tb_dados_instituicao_idDadosInsitiuicao,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email
FROM
     `tb_usuario` tb_usuario INNER JOIN `entrada_tesouraria` entrada_tesouraria ON tb_usuario.`codigo` = entrada_tesouraria.`fk_usuario`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     entrada_tesouraria.`data_entrada`  BETWEEN  $P{SUB_PARM_DATA_INICIO} AND  $P{SUB_PARM_DATA_FIM}]]>
	</queryString>
