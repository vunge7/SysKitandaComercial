	<parameter name="CODIGO_VENDA" class="java.lang.Integer"/>
	<parameter name="PARM_MESA" class="java.lang.Integer"/>
	<parameter name="PARM_LUGAR" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select 
	p.designacao ,
	sum(i.quantidade) as QTD_TOTAL ,
	pc.preco_venda ,
	i.valor_iva ,
	i.total ,
	l.designacao  AS lugar
from tb_item_venda i
inner join tb_mesas m on m.pk_mesas  = i.fk_mesas 
inner join tb_lugares l on l.pk_lugares  = i.fk_lugares 
inner join tb_preco pc on pc.pk_preco  = i.fk_preco 
inner join tb_produto p on p.codigo = i.codigo_produto
WHERE
	i.codigo_venda  = $P{CODIGO_VENDA}
	and m.pk_mesas  =  $P{PARM_MESA}
 	and l.pk_lugares = $P{PARM_LUGAR}
group by p.codigo ;]]>
	</queryString>
