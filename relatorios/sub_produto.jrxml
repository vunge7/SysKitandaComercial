	<variable name="SOMA_PRECO_COMPRA" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{tb_stock_quantidade_existente}*$F{MAX_PRECO_COMPRA}.doubleValue()]]></variableExpression>
