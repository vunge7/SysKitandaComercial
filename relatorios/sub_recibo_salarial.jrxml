	<queryString>
		<![CDATA[SELECT
     item_recibo_rh.`descricao` AS item_recibo_rh_descricao,
     item_recibo_rh.`remuneracao` AS item_recibo_rh_remuneracao,
     item_recibo_rh.`desconto` AS item_recibo_rh_desconto,
     item_recibo_rh.`fk_recibo_rh` AS item_recibo_rh_fk_recibo_rh,
     tb_funcionario.`numero_funcionario` AS tb_funcionario_numero_funcionario,
     tb_funcionario.`nome` AS tb_funcionario_nome,
     recibo_rh.`pk_recibo_rh` AS recibo_rh_pk_recibo_rh,
     recibo_rh.`data_hora` AS recibo_rh_data_hora,
     recibo_rh.`total_remuneracao` AS recibo_rh_total_remuneracao,
     recibo_rh.`total_desconto` AS recibo_rh_total_desconto,
     recibo_rh.`total_pago` AS recibo_rh_total_pago,
     recibo_rh.`fk_funcionario` AS recibo_rh_fk_funcionario,
     tb_funcao.`designacao` AS tb_funcao_designacao,
     tb_funcionario.`n_seguranca_social` AS tb_funcionario_n_seguranca_social,
     recibo_rh.`periodo` AS recibo_rh_periodo,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     fecho_periodo.`pk_fecho_periodo` AS fecho_periodo_pk_fecho_periodo,
     fecho_periodo.`data_abertura` AS fecho_periodo_data_abertura,
     fecho_periodo.`data_fecho` AS fecho_periodo_data_fecho,
     recibo_rh.`vencimento` AS recibo_rh_vencimento,
     recibo_rh.`dias_trabalhado` AS recibo_rh_dias_trabalhado,
     forma_pagamento.`designacao` AS forma_pagamento_designacao
FROM
     `tb_funcionario` tb_funcionario INNER JOIN `recibo_rh` recibo_rh ON tb_funcionario.`idFuncionario` = recibo_rh.`fk_funcionario`
     INNER JOIN `item_recibo_rh` item_recibo_rh ON recibo_rh.`pk_recibo_rh` = item_recibo_rh.`fk_recibo_rh`
     INNER JOIN `fecho_periodo` fecho_periodo ON recibo_rh.`fk_fecho_periodo` = fecho_periodo.`pk_fecho_periodo`
     INNER JOIN `forma_pagamento` forma_pagamento ON recibo_rh.`fk_forma_pagamento` = forma_pagamento.`pk_forma_pagamento`
     INNER JOIN `tb_funcao` tb_funcao ON tb_funcionario.`fk_funcao` = tb_funcao.`pk_funcao`
     inner join tb_mes_rh m on m.pk_mes_rh = `fecho_periodo`.fk_periodo
     inner join tb_ano a on a.idAno  = fecho_periodo.fk_ano
     inner join `tb_dados_instituicao` tb_dados_instituicao
where
 a.descrisao  = $P{ANO} and
 m.descricao  = $P{PERIODO} and
 recibo_rh.`fk_funcionario` = $P{ID_FUNCIONARIO} ;]]>
	</queryString>
