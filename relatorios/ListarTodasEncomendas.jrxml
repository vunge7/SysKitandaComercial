	<queryString>
		<![CDATA[SELECT
     tb_dados_instituicao.`nome` AS NOME_INSTITUICAO,
     tb_dados_instituicao.`telefone` AS TELEFONE,
     tb_dados_instituicao.`enderecos` AS ENDERECO,
     tb_dados_instituicao.`email` AS EMAIL,
     tb_dados_instituicao.`nif` AS NIF,
     tb_clientes_encomenda.`nome_cliente` AS tb_clientes_encomenda_nome_cliente,
     tb_clientes_encomenda.`saldo` AS tb_clientes_encomenda_saldo,
     tb_clientes_encomenda.`credito` AS tb_clientes_encomenda_credito,
     tb_clientes_encomenda.`idCliente` AS tb_clientes_encomenda_idCliente,
     tb_encomenda.`total_encomenda` AS tb_encomenda_total_encomenda,
     tb_encomenda.`status_entrega` AS tb_encomenda_status_entrega,
     tb_encomenda.`data_encomenda` AS tb_encomenda_data_encomenda,
     tb_encomenda.`data_entrega_prevista` AS tb_encomenda_data_entrega_prevista,
     tb_encomenda.`idEncomenda` AS tb_encomenda_idEncomenda
FROM
     `tb_clientes_encomenda` tb_clientes_encomenda INNER JOIN `tb_encomenda` tb_encomenda ON tb_clientes_encomenda.`idCliente` = tb_encomenda.`idCliente`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE tb_clientes_encomenda.`idCliente` =  $P{ID_CLIENTE}

ORDER BY tb_encomenda.`idEncomenda`]]>
	</queryString>
