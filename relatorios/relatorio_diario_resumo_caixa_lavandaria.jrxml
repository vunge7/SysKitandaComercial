	<queryString>
		<![CDATA[select
   u.codigo ,
   u.nome,
   d.nome  as nome_instituicao,
   d.nif  as nif_instituicao,
   d.enderecos  as enderecos_inituicao
from tb_venda v
inner join tb_usuario u on u.codigo  = v.codigo_usuario
inner join tb_dados_instituicao d
where v.dataVenda between $P{DATA_1} and $P{DATA_2}
	and  v.fk_documento = $P{ID_DOCUMENTO}
	and  v.performance = 'false'
	and  v.credito = 'false'
	and  v.status_eliminado = 'false'
group  by u.codigo;]]>
	</queryString>
