	<queryString>
		<![CDATA[SELECT
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_stock.`cod_produto_codigo` AS tb_stock_cod_produto_codigo,
     tb_stock.`quantidade_existente` AS tb_stock_quantidade_existente,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_stock.`quant_critica` AS tb_stock_quant_critica,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_stock.`cod_armazem` AS tb_stock_cod_armazem,
     tb_fornecedor.`nome` AS tb_fornecedor_nome,
     tb_fornecedor.`codigo` AS tb_fornecedor_codigo
FROM
     `tb_produto` tb_produto INNER JOIN `tb_stock` tb_stock ON tb_produto.`codigo` = tb_stock.`cod_produto_codigo`
     INNER JOIN `tb_armazem` tb_armazem ON tb_stock.`cod_armazem` = tb_armazem.`codigo`
     INNER JOIN `tb_fornecedor` tb_fornecedor ON tb_produto.`cod_fornecedores` = tb_fornecedor.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_stock.`quant_critica` >= tb_stock.`quantidade_existente` AND tb_stock.`cod_armazem` = $P{ID_ARMAZEM} AND tb_fornecedor.`codigo` = $P{ID_FORNECEDOR}]]>
	</queryString>
