	<parameter name="CODIGO_ENTRADA_VASILHAME" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT
     tb_vasilhame.`descricao` AS tb_vasilhame_descricao,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_saida_vasilhame.`data_saida` AS tb_saida_vasilhame_data_saida,
     tb_saida_vasilhame.`hora_saida` AS tb_saida_vasilhame_hora_saida,
     tb_saida_vasilhame.`pk_saida_vasilhame` AS tb_saida_vasilhame_pk_saida_vasilhame,
     tb_saida_vasilhame.`nome_funcionario` AS tb_saida_vasilhame_nome_funcionario,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_item_saida_vasilhame.`qtd` AS tb_item_saida_vasilhame_qtd,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_vasilhame.`pk_vasilhame` AS tb_vasilhame_pk_vasilhame
FROM
     `tb_saida_vasilhame` tb_saida_vasilhame INNER JOIN `tb_item_saida_vasilhame` tb_item_saida_vasilhame ON tb_saida_vasilhame.`pk_saida_vasilhame` = tb_item_saida_vasilhame.`fk_saida_vazilhame`
     INNER JOIN `tb_armazem` tb_armazem ON tb_saida_vasilhame.`fk_armazem` = tb_armazem.`codigo`
     INNER JOIN `tb_usuario` tb_usuario ON tb_saida_vasilhame.`fk_usuario` = tb_usuario.`codigo`
     INNER JOIN `tb_vasilhame` tb_vasilhame ON tb_item_saida_vasilhame.`fk_vasihame` = tb_vasilhame.`pk_vasilhame`
     INNER JOIN `tb_produto` tb_produto ON tb_vasilhame.`fk_produto` = tb_produto.`codigo`
WHERE
     tb_saida_vasilhame.`pk_saida_vasilhame` = $P{CODIGO_ENTRADA_VASILHAME}]]>
	</queryString>
