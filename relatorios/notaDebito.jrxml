	<queryString>
		<![CDATA[SELECT
     tb_cliente.`nome` AS tb_cliente_nome,
     tb_produto.`designacao` AS tb_produto_designacao,
     notas_item.`total` AS notas_item_total,
     notas.`total_venda` AS notas_total_venda,
notas.cod_nota As cod_nota,
notas.ref_cod_fact As ref_cod_fact,
     tb_produto.`codigo` AS tb_produto_codigo,
     notas_item.`quantidade` AS notas_item_qauntidade,
     notas.`pk_nota` AS notas_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_produto.`preco_venda` AS tb_produto_preco_venda,
     notas.`troco` AS notas_troco,
     notas.`valor_entregue` AS notas_valor_entregue,
     notas.`dataNota` AS notas_dataVenda,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_armazem.`codigo` AS tb_armazem_codigo,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     notas.`nome_cliente` AS notas_nome_cliente,
     notas.`hora` AS notas_hora,
     tb_preco.`preco_venda` AS tb_preco_preco_venda,
     notas.`desconto_total` AS notas_desconto_total,
     notas.`total_geral` AS notas_total_geral,
     tb_cliente.`morada` AS tb_cliente_morada,
     tb_cliente.`telefone` AS tb_cliente_telefone,
     tb_cliente.`nif` AS tb_cliente_nif,
     notas_item.`valor_iva` AS notas_item_valor_iva,
     notas_item.`desconto` AS notas_item_desconto,
     notas.`cod_fact` AS notas_cod_fact,
     notas.`assinatura` AS notas_assinatura,
     notas.`obs` AS notas_obs,
     notas.`hash_cod` AS notas_hash_cod,
     notas.`total_iva` AS notas_total_iva,
     notas.`performance` AS notas_performance,
     cambio.`pk_cambio` AS cambio_pk_cambio,
     cambio.`valor` AS cambio_valor,
     cambio.`data_hora` AS cambio_data_hora,
     cambio.`fk_moeda` AS cambio_fk_moeda,
     moeda.`pk_moeda` AS moeda_pk_moeda,
     moeda.`designacao` AS moeda_designacao,
     moeda.`abreviacao` AS moeda_abreviacao,
     tb_banco.`idBanco` AS tb_banco_idBanco,
     tb_banco.`descrisao` AS tb_banco_descrisao,
     tb_banco.`numero` AS tb_banco_numero,
     tb_banco.`saldo_banco` AS tb_banco_saldo_banco,
     notas.`total_por_extenso` AS notas_total_por_extenso,
     notas.motivo as motivo,
     ano_economico.`pk_ano_economico` AS ano_economico_pk_ano_economico,
     ano_economico.`designacao` AS ano_economico_designacao,
     ano_economico.`serie` AS ano_economico_serie,
     ano_economico.`data_inicio` AS ano_economico_data_inicio,
     ano_economico.`data_fim` AS ano_economico_data_fim,
     tb_cliente.`email` AS tb_cliente_email,
     documento.`pk_documento` AS documento_pk_documento,
     documento.`designacao` AS documento_designacao,
     documento.`abreviacao` AS documento_abreviacao,
     documento.`cod_ultimo_doc` AS documento_cod_ultimo_doc,
     documento.`descricao_ultimo_doc` AS documento_descricao_ultimo_doc,
     documento.`ultima_data` AS documento_ultima_data,

    tb_venda.valor_entregue ref_valor_entregue,
    tb_venda.total_venda ref_valor_entregue,
    tb_venda.total_iva ref_imposto,
    tb_venda.dataVenda ref_data_fact,
    tb_venda.total_venda ref_total_venda,
    tb_venda.hora as ref_hora
FROM
     `tb_produto` tb_produto INNER JOIN `notas_item` notas_item ON tb_produto.`codigo` = notas_item.`fk_produto`
     INNER JOIN `notas` notas ON notas_item.`fk_nota` = notas.`pk_nota`
     INNER JOIN `tb_preco` tb_preco ON notas_item.`fk_preco` = tb_preco.`pk_preco`
     INNER JOIN `tb_cliente` tb_cliente ON notas.`codigo_cliente` = tb_cliente.`codigo`
     INNER JOIN `tb_usuario` tb_usuario ON notas.`codigo_usuario` = tb_usuario.`codigo`
     INNER JOIN `tb_armazem` tb_armazem ON notas.`idArmazemFK` = tb_armazem.`codigo`
     INNER JOIN `cambio` cambio ON notas.`fk_cambio` = cambio.`pk_cambio`
     INNER JOIN `tb_banco` tb_banco ON notas.`idBanco` = tb_banco.`idBanco`
     INNER JOIN `ano_economico` ano_economico ON notas.`fk_ano_economico` = ano_economico.`pk_ano_economico`
     INNER JOIN `documento` documento ON notas.`fk_documento` = documento.`pk_documento`
     INNER JOIN `moeda` moeda ON cambio.`fk_moeda` = moeda.`pk_moeda`
     INNER JOIN `tb_venda` tb_venda ON tb_venda.`cod_fact` = notas.ref_cod_fact,
     `tb_dados_instituicao` tb_dados_instituicao


WHERE
     notas.`pk_nota` = $P{CODIGO_NOTA}]]>
	</queryString>
