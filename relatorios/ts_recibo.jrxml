	<parameter name="COD_OPERACAO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT cm.*, di.nome as nome, di.telefone as telefone, di.enderecos as endereco, di.email as email, di.nif as nif,
co.tipo as tipo_operacoes, co.data_hora AS data_operacoes, co.valor AS valor_operacoes, co.user_name, co.beneficiario
 FROM conta_movimentos cm
INNER JOIN contas c ON c.pk_contas = cm.conta_id
INNER JOIN conta_operacoes co ON co.pk_conta_operacao = cm.cod_operacao
INNER JOIN tb_dados_instituicao di
WHERE co.pk_conta_operacao = $P{COD_OPERACAO}]]>
	</queryString>
