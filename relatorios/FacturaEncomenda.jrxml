	<parameter name="N_ENCOMENDA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_item_encomenda.`total` AS tb_item_encomenda_total,
     tb_item_encomenda.`quantidade` AS tb_item_encomenda_quantidade,
     tb_encomenda.`data_encomenda` AS tb_encomenda_data_encomenda,
     tb_encomenda.`data_entrega_prevista` AS tb_encomenda_data_entrega_prevista,
     tb_encomenda.`total_encomenda` AS tb_encomenda_total_encomenda,
     tb_clientes_encomenda.`nome_cliente` AS tb_clientes_encomenda_nome_cliente,
     tb_clientes_encomenda.`saldo` AS tb_clientes_encomenda_saldo,
     tb_clientes_encomenda.`credito` AS tb_clientes_encomenda_credito,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_encomenda.`obs` AS tb_encomenda_obs,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_encomenda.`idEncomenda` AS tb_encomenda_idEncomenda,
     tb_encomenda.`status_entrega` AS tb_encomenda_status_entrega,
     tb_stock.`preco_venda` AS tb_stock_preco_venda,
     tb_stock.`cod_armazem` AS tb_stock_cod_armazem
FROM
     `tb_encomenda` tb_encomenda INNER JOIN `tb_item_encomenda` tb_item_encomenda ON tb_encomenda.`idEncomenda` = tb_item_encomenda.`idEncomenda`
     INNER JOIN `tb_clientes_encomenda` tb_clientes_encomenda ON tb_encomenda.`idCliente` = tb_clientes_encomenda.`idCliente`
     INNER JOIN `tb_usuario` tb_usuario ON tb_encomenda.`idUsuario` = tb_usuario.`codigo`
     INNER JOIN `tb_produto` tb_produto ON tb_item_encomenda.`idProduto` = tb_produto.`codigo`
     INNER JOIN `tb_stock` tb_stock ON tb_produto.`codigo` = tb_stock.`cod_produto_codigo`
     INNER JOIN `tb_armazem` tb_armazem ON tb_stock.`cod_armazem` = tb_armazem.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_encomenda.`idEncomenda` = $P{N_ENCOMENDA} AND  tb_stock.`cod_armazem` = 2]]>
	</queryString>
