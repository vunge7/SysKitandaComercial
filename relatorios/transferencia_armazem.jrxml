	<parameter name="COD_SAIDA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     tb_item_saidas.`quantidade` AS tb_item_saidas_quantidade,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_saidas_produtos.`pk_saidas_produtos` AS tb_saidas_produtos_pk_saidas_produtos,
     tb_saidas_produtos.`data_saida` AS tb_saidas_produtos_data_saida,
     tb_saidas_produtos.`obs` AS tb_saidas_produtos_obs,
     tb_saidas_produtos.`nome_funcionario` AS tb_saidas_produtos_nome_funcionario,
     tb_saidas_produtos.`hora_saida` AS tb_saidas_produtos_hora_saida,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_produto.`codigo` AS tb_produto_codigo
FROM
     `tb_produto` tb_produto INNER JOIN `tb_item_saidas` tb_item_saidas ON tb_produto.`codigo` = tb_item_saidas.`fk_produtos`
     INNER JOIN `tb_saidas_produtos` tb_saidas_produtos ON tb_item_saidas.`fk_saidas_produtos` = tb_saidas_produtos.`pk_saidas_produtos`
     INNER JOIN `tb_usuario` tb_usuario ON tb_saidas_produtos.`fk_usuario` = tb_usuario.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_saidas_produtos.`pk_saidas_produtos` = $P{COD_SAIDA}]]>
	</queryString>
