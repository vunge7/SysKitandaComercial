	<queryString>
		<![CDATA[SELECT DISTINCT
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_tipo_produto.`codigo` AS tb_tipo_produto_codigo,
     tb_tipo_produto.`designacao` AS tb_tipo_produto_designacao,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     familia.`pk_familia` AS familia_pk_familia,
     familia.`designacao` AS familia_designacao
FROM
     `familia` familia INNER JOIN `tb_tipo_produto` tb_tipo_produto ON familia.`pk_familia` = tb_tipo_produto.`fk_familia`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE familia.`pk_familia` <> 1
ORDER BY
     tb_tipo_produto.`designacao` ASC]]>
	</queryString>
