	<queryString>
		<![CDATA[SELECT
     forma_pagamento_item.`pk_forma_pagamento_item` AS forma_pagamento_item_pk_forma_pagamento_item,
     forma_pagamento_item.`valor` AS forma_pagamento_item_valor,
     forma_pagamento_item.`referencia` AS forma_pagamento_item_referencia,
     forma_pagamento_item.`fk_forma_pagamento` AS forma_pagamento_item_fk_forma_pagamento,
     forma_pagamento_item.`fk_venda` AS forma_pagamento_item_fk_venda,
     forma_pagamento.`pk_forma_pagamento` AS forma_pagamento_pk_forma_pagamento,
     forma_pagamento.`descrisao` AS forma_pagamento_descrisao,
     tb_venda.`codigo` AS tb_venda_codigo,
     tb_venda.`dataVenda` AS tb_venda_dataVenda,
     tb_venda.`performance` AS tb_venda_performance,
     tb_venda.`credito` AS tb_venda_credito,
     tb_venda.`hora` AS tb_venda_hora,
     tb_venda.`status_eliminado` AS tb_venda_status_eliminado,
     tb_venda.`cod_fact` AS tb_venda_cod_fact,
     tb_venda.`idArmazemFK` AS tb_venda_idArmazemFK,
     tb_armazem.`codigo` AS tb_armazem_codigo,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_cliente.`nome` AS tb_cliente_nome,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos
FROM
     `forma_pagamento` forma_pagamento INNER JOIN `forma_pagamento_item` forma_pagamento_item ON forma_pagamento.`pk_forma_pagamento` = forma_pagamento_item.`fk_forma_pagamento`
     INNER JOIN `tb_venda` tb_venda ON forma_pagamento_item.`fk_venda` = tb_venda.`codigo`
     INNER JOIN `tb_armazem` tb_armazem ON tb_venda.`idArmazemFK` = tb_armazem.`codigo`
     INNER JOIN `tb_usuario` tb_usuario ON tb_venda.`codigo_usuario` = tb_usuario.`codigo`
     INNER JOIN `tb_cliente` tb_cliente ON tb_venda.`codigo_cliente` = tb_cliente.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE DATE(tb_venda.`dataVenda`) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2} AND tb_armazem.`codigo` = $P{PARM_ARMAZEM} AND forma_pagamento.`pk_forma_pagamento` = $P{PARM_FORMA} ORDER BY  tb_venda.`cod_fact` ASC]]>
	</queryString>
