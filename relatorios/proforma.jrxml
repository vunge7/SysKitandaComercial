	<parameter name="CODIGO_PROFORMA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     tb_cliente.`nome` AS tb_cliente_nome,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_produto.`preco_venda` AS tb_produto_preco_venda,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_pro_forma.`pk_pro_forma` AS tb_pro_forma_pk_pro_forma,
     tb_pro_forma.`data` AS tb_pro_forma_data,
     tb_pro_forma.`hora` AS tb_pro_forma_hora,
     tb_pro_forma.`fk_cliente` AS tb_pro_forma_fk_cliente,
     tb_pro_forma.`fk_usuario` AS tb_pro_forma_fk_usuario,
     tb_pro_forma.`total` AS tb_pro_forma_total,
     tb_item_pro_forma.`pk_item_pro_forma` AS tb_item_pro_forma_pk_item_pro_forma,
     tb_item_pro_forma.`qtd` AS tb_item_pro_forma_qtd,
     tb_preco.`preco_venda` AS tb_preco_preco_venda
FROM
     `tb_cliente` tb_cliente INNER JOIN `tb_pro_forma` tb_pro_forma ON tb_cliente.`codigo` = tb_pro_forma.`fk_cliente`
     INNER JOIN `tb_usuario` tb_usuario ON tb_pro_forma.`fk_usuario` = tb_usuario.`codigo`
     INNER JOIN `tb_item_pro_forma` tb_item_pro_forma ON tb_pro_forma.`pk_pro_forma` = tb_item_pro_forma.`fk_pro_forma`
     INNER JOIN `tb_produto` tb_produto ON tb_item_pro_forma.`fk_produto` = tb_produto.`codigo`
     INNER JOIN `tb_preco` tb_preco ON tb_item_pro_forma.`fk_preco` = tb_preco.`pk_preco`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_pro_forma.`pk_pro_forma` = $P{CODIGO_PROFORMA}]]>
	</queryString>
