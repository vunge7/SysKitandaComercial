	<queryString>
		<![CDATA[SELECT
produto.codigo,
produto.designacao ,
 SUM(item.quantidade) AS QTD
FROM
tb_item_venda item,
tb_venda venda,
tb_produto produto
WHERE item.codigo_venda = venda.codigo AND item.codigo_produto = produto.codigo
      AND DATE(venda.dataVenda) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2}
 AND venda.`idArmazemFK` = $P{PARM_ID_ARMAZEM} AND venda.fk_documento IN(1,2)
 AND venda.`status_eliminado` = 'false'
 GROUP BY produto.codigo]]>
	</queryString>
