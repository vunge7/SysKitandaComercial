	<queryString>
		<![CDATA[select  
	date(v.dataVenda) as data_venda, 
	sum(i.quantidade) as qtd 
from tb_item_venda i 
inner join tb_venda v on v.codigo  = i.codigo_venda 
inner join tb_produto p on p.codigo = i.codigo_produto 
WHERE  p.codigo = $P{PARM_ID_PRODUTO} AND date(v.dataVenda) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2}
and v.status_eliminado = 'false'
and v.fk_documento IN(1,2)
group by date(v.dataVenda);]]>
	</queryString>
	<field name="data_venda" class="java.sql.Date"/>
	<field name="qtd" class="java.lang.Double">
