	<queryString>
		<![CDATA[SELECT
     caixa.`pk_caixa` AS caixa_pk_caixa,
     caixa.`data_abertura` AS caixa_data_abertura,
     caixa.`data_fecho` AS caixa_data_fecho,
     caixa.`total_vendas` AS caixa_total_vendas,
     caixa.`numero_vendas` AS caixa_numero_vendas,
     caixa.`valor_inicial` AS caixa_valor_inicial,
     caixa.`usuario_fecho` AS caixa_usuario_fecho,
     caixa.`usuario_abertura` AS caixa_usuario_abertura,
     item_caixa.`pk_item_caixa` AS item_caixa_pk_item_caixa,
     item_caixa.`valor_declarado` AS item_caixa_valor_declarado,
     item_caixa.`valor_real` AS item_caixa_valor_real,
     forma_pagamento.`designacao` AS forma_pagamento_designacao
FROM
     `caixa` caixa INNER JOIN `item_caixa` item_caixa ON caixa.`pk_caixa` = item_caixa.`fk_caixa`
     INNER JOIN `forma_pagamento` forma_pagamento ON item_caixa.`fk_forma_pagamento` = forma_pagamento.`pk_forma_pagamento`
WHERE
     caixa.pk_caixa = $P{ID_CAIXA}]]>
	</queryString>
