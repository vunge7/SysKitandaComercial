	<queryString>
		<![CDATA[SELECT
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_usuario.`codigo` AS tb_usuario_codigo,
     tb_usuario.`telefone` AS tb_usuario_telefone,
     tb_usuario.`email` AS tb_usuario_email,
     tb_usuario.`endereco` AS tb_usuario_endereco,
     tb_sexo.`designacao` AS tb_sexo_designacao,
     tb_usuario.`status` AS tb_usuario_status
FROM
     `tb_sexo` tb_sexo INNER JOIN `tb_usuario` tb_usuario ON tb_sexo.`codigo` = tb_usuario.`codigo_sexo`
WHERE
 	tb_usuario.`status` = $P{STATUS}

ORDER BY    tb_usuario.`nome` ASC]]>
