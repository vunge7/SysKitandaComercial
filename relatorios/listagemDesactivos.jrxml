	<queryString>
		<![CDATA[SELECT
     tb_funcionario.`nome` AS tb_funcionario_nome,
     tb_funcionario.`idFuncionario` AS tb_funcionario_idFuncionario,
     tb_funcionario.`telefone` AS tb_funcionario_telefone,
     tb_funcionario.`data_nascimento` AS tb_funcionario_data_nascimento,
     tb_departamento.`designacao` AS tb_departamento_designacao,
     tb_estado_civil.`designacao` AS tb_estado_civil_designacao,
     tb_funcao.`designacao` AS tb_funcao_designacao,
     tb_status.`idStatus` AS tb_status_idStatus,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome
FROM
     `tb_departamento` tb_departamento INNER JOIN `tb_funcionario` tb_funcionario ON tb_departamento.`pk_departamento` = tb_funcionario.`fk_departamento`
     INNER JOIN `tb_estado_civil` tb_estado_civil ON tb_funcionario.`fk_estado_civil` = tb_estado_civil.`pk_estado_civil`
     INNER JOIN `tb_funcao` tb_funcao ON tb_funcionario.`fk_funcao` = tb_funcao.`pk_funcao`
     INNER JOIN `tb_status` tb_status ON tb_funcionario.`idStatusFK` = tb_status.`idStatus`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_status.`idStatus` = 2
ORDER BY
     tb_funcionario.`nome` ASC]]>
	</queryString>
