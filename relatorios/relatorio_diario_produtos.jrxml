	<queryString>
		<![CDATA[SELECT
    p.codigo,
    p.designacao,
    pr.preco_venda, -- preço de venda do momento da transação
    SUM(iv.quantidade) AS QTD,
    SUM(iv.total) AS total_vendido
FROM tb_item_venda iv
JOIN tb_venda v     ON iv.codigo_venda = v.codigo
JOIN tb_produto p   ON iv.codigo_produto = p.codigo
JOIN tb_preco pr    ON iv.fk_preco = pr.pk_preco
WHERE DATE(v.dataVenda) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2}
  AND v.idArmazemFK = $P{PARM_ID_ARMAZEM}
  AND v.fk_documento IN (1,2)
  AND v.status_eliminado = 'false'
GROUP BY p.codigo, p.designacao, pr.preco_venda
ORDER BY p.designacao;]]>
	</queryString>
