	<parameter name="PARM_DATA_1" class="java.util.Date"/>
	<parameter name="PARM_DATA_2" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_armazem.`codigo` AS tb_armazem_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_estorno.`pk_estorno` AS tb_estorno_pk_estorno,
     tb_estorno.`data_estorno` AS tb_estorno_data_estorno,
     tb_estorno.`fk_usuario` AS tb_estorno_fk_usuario,
     tb_estorno.`idArmazemFK` AS tb_estorno_idArmazemFK,
     tb_estorno.`hora_estorno` AS tb_estorno_hora_estorno,
     tb_estorno.`status_eliminado` AS tb_estorno_status_eliminado,
     tb_estorno.`obs` AS tb_estorno_obs,
     tb_item_estorno.`codigo` AS tb_item_estorno_codigo,
     tb_item_estorno.`fk_estorno` AS tb_item_estorno_fk_estorno,
     tb_item_estorno.`fk_produtos` AS tb_item_estorno_fk_produtos,
     tb_item_estorno.`quantidade` AS tb_item_estorno_quantidade
FROM
     `tb_armazem` tb_armazem INNER JOIN `tb_estorno` tb_estorno ON tb_armazem.`codigo` = tb_estorno.`idArmazemFK`
     INNER JOIN `tb_usuario` tb_usuario ON tb_estorno.`fk_usuario` = tb_usuario.`codigo`
     INNER JOIN `tb_item_estorno` tb_item_estorno ON tb_estorno.`pk_estorno` = tb_item_estorno.`fk_estorno`
     INNER JOIN `tb_produto` tb_produto ON tb_item_estorno.`fk_produtos` = tb_produto.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
      tb_estorno.`data_estorno`  BETWEEN  $P{PARM_DATA_1} AND $P{PARM_DATA_2}
     AND tb_estorno.`status_eliminado` = 'false']]>
	</queryString>
