	<parameter name="CODIGO_ITEM_PEDIDO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     tb_item_pedidos.`qtd` AS tb_item_pedidos_qtd,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_lugares.`designacao` AS tb_lugares_designacao,
     tb_mesas.`designacao` AS tb_mesas_designacao,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_item_pedidos.`fk_pedidos` AS tb_item_pedidos_fk_pedidos,
     tb_item_pedidos.`pk_item_pedidos` AS tb_item_pedidos_pk_item_pedidos
FROM
     `tb_produto` tb_produto INNER JOIN `tb_item_pedidos` tb_item_pedidos ON tb_produto.`codigo` = tb_item_pedidos.`fk_produtos`
     INNER JOIN `tb_lugares` tb_lugares ON tb_item_pedidos.`fk_lugares` = tb_lugares.`pk_lugares`
     INNER JOIN `tb_pedido` tb_pedido ON tb_item_pedidos.`fk_pedidos` = tb_pedido.`pk_pedido`
     INNER JOIN `tb_mesas` tb_mesas ON tb_pedido.`fk_mesas` = tb_mesas.`pk_mesas`
WHERE
     tb_item_pedidos.`pk_item_pedidos` = $P{CODIGO_ITEM_PEDIDO}]]>
	</queryString>
