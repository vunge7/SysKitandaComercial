	<parameter name="DOC_OPERACAO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT *, di.nome as nome, di.telefone as telefone, di.enderecos as endereco, di.email as email, di.nif as nif,
co.tipo as tipo_operacoes, co.data_hora AS data_operacoes, co.valor AS valor_operacoes FROM conta_movimentos cm
INNER JOIN contas c ON c.pk_contas = cm.conta_id
INNER JOIN conta_operacoes co ON co.pk_conta_operacao = cm.cod_operacao
INNER JOIN tb_dados_instituicao di
WHERE co.pk_conta_operacao = $P{DOC_OPERACAO}
]]>
	</queryString>
