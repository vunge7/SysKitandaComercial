	<parameter name="DATA_1" class="java.sql.Timestamp"/>
	<parameter name="DATA_2" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[select
	f.designacao as forma ,
	SUM(i.valor_real) as valor
from forma_pagamento_item i
inner join tb_venda v on v.codigo  = i.fk_venda
inner join forma_pagamento f on f.pk_forma_pagamento  = i.fk_forma_pagamento
where v.dataVenda   BETWEEN $P{DATA_1} AND $P{DATA_2}
group by f.designacao
order by f.pk_forma_pagamento ASC]]>
	</queryString>
