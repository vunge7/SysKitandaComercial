	<parameter name="COD_ENTRADA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
     tb_entrada.`idEntrada` AS tb_entrada_idEntrada,
     tb_entrada.`data_entrada` AS tb_entrada_data_entrada,
     tb_entrada.`idUsuario` AS tb_entrada_idUsuario,
     tb_entrada.`idArmazemFK` AS tb_entrada_idArmazemFK,
     tb_entrada.`status_eliminado` AS tb_entrada_status_eliminado,
     tb_item_entradas.`id_item_entradas` AS tb_item_entradas_id_item_entradas,
     tb_item_entradas.`id_produto` AS tb_item_entradas_id_produto,
     tb_item_entradas.`quantidade` AS tb_item_entradas_quantidade,
     tb_item_entradas.`fk_entradas` AS tb_item_entradas_fk_entradas,
     tb_usuario.`codigo` AS tb_usuario_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_usuario.`senha` AS tb_usuario_senha,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_produto.`preco` AS tb_produto_preco,
     tb_produto.`data_fabrico` AS tb_produto_data_fabrico,
     tb_produto.`data_expiracao` AS tb_produto_data_expiracao,
     tb_produto.`codBarra` AS tb_produto_codBarra,
     tb_produto.`status` AS tb_produto_status,
     tb_produto.`data_entrada` AS tb_produto_data_entrada,
     tb_produto.`stocavel` AS tb_produto_stocavel,
     tb_produto.`preco_venda` AS tb_produto_preco_venda,
     tb_produto.`percentagem_desconto` AS tb_produto_percentagem_desconto,
     tb_produto.`quantidade_desconto` AS tb_produto_quantidade_desconto,
     tb_produto.`codigo_manual` AS tb_produto_codigo_manual,
     tb_produto.`cod_unidade` AS tb_produto_cod_unidade,
     tb_produto.`cod_local` AS tb_produto_cod_local,
     tb_produto.`cod_fornecedores` AS tb_produto_cod_fornecedores,
     tb_produto.`cod_Tipo_Produto` AS tb_produto_cod_Tipo_Produto,
     tb_produto.`fk_modelo` AS tb_produto_fk_modelo,
     tb_produto.`fk_grupo` AS tb_produto_fk_grupo,
     tb_produto.`photo` AS tb_produto_photo,
     tb_produto.`status_iva` AS tb_produto_status_iva,
     tb_produto.`cozinha` AS tb_produto_cozinha,
     tb_produto.`cod_pai` AS tb_produto_cod_pai,
     tb_produto.`unidade_compra` AS tb_produto_unidade_compra
FROM
     `tb_entrada` tb_entrada INNER JOIN `tb_item_entradas` tb_item_entradas ON tb_entrada.`idEntrada` = tb_item_entradas.`fk_entradas`
     INNER JOIN `tb_usuario` tb_usuario ON tb_entrada.`idUsuario` = tb_usuario.`codigo`
     INNER JOIN `tb_produto` tb_produto ON tb_item_entradas.`id_produto` = tb_produto.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_entrada.`idEntrada` = $P{COD_ENTRADA}]]>
