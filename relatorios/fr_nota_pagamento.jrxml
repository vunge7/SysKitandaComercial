	<queryString>
		<![CDATA[select  c.pk_compra,  
		c.doc_vosso_data, 
		c.doc_vosso_data_vencimento,
		c.doc_vosso,
		c.doc_vosso_numero,
		c.total_compra, 
		c.valor_por_pagar, 
		c.valor_pago,
		d.nome as instituicao_nome,
		d.enderecos as instituicao_endereco,
		d.nif as instituicao_nif,
		d.telefone as instituicao_telefone,
		d.email as instituicao_email,
		f.nome as fornecedor_nome ,
		f.endereco as fornecedor_endereco,
		f.telefone as fornecedor_telefone,
		f.email as fornecedor_email
from compras c
inner join tb_dados_instituicao d
inner join tb_fornecedor f on f.codigo = c.fk_fornecedor
where c.doc_vosso_numero = $P{DOC_NUMERO} AND f.codigo = $P{COD_FORNECEDOR};]]>
	</queryString>
