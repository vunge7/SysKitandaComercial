	<queryString>
		<![CDATA[SELECT
     a.`designacao` AS tb_armazem_designacao,
     e.`data_entrada` AS tb_entrada_data_entrada,
     a.`codigo` AS tb_armazem_codigo,
     e.`idArmazemFK` AS tb_entrada_idArmazemFK,
     u.`nome` AS NOME_USUARIO,
     e.`idEntrada` AS e_idEntrada
FROM
     `tb_entrada` e,
     `tb_armazem` a,
     `tb_usuario` u
WHERE
e.idArmazemFK = a.codigo AND
e.idUsuario = u.codigo AND
e.idArmazemFK = $P{ID_ARMAZEM} AND
e.data_entrada BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}]]>
	</queryString>
