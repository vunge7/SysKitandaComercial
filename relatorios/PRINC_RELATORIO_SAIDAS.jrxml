	<queryString>
		<![CDATA[SELECT
     tb_usuario.`nome` AS tb_usuario_nome,
     saidas_tesouraria.`descricao` AS saidas_tesouraria_descricao,
     saidas_tesouraria.`pk_saidas_tesouraria` AS saidas_tesouraria_pk_saidas_tesouraria,
     saidas_tesouraria.`data_saida` AS saidas_tesouraria_data_saida,
     saidas_tesouraria.`valor` AS saidas_tesouraria_valor,
     saidas_tesouraria.`hora_saida` AS saidas_tesouraria_hora_saida,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_dados_instituicao.`telefone` AS tb_dados_instituicao_telefone,
     tb_dados_instituicao.`enderecos` AS tb_dados_instituicao_enderecos,
     tb_dados_instituicao.`email` AS tb_dados_instituicao_email,
     tb_dados_instituicao.`nif` AS tb_dados_instituicao_nif
FROM
     `tb_usuario` tb_usuario INNER JOIN `saidas_tesouraria` saidas_tesouraria ON tb_usuario.`codigo` = saidas_tesouraria.`fk_usuario`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     saidas_tesouraria.`data_saida`  BETWEEN  $P{SUB_PARM_DATA_INICIO} AND  $P{SUB_PARM_DATA_FIM}]]>
	</queryString>
