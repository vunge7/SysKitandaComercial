	<queryString>
		<![CDATA[select tb_dados_instituicao.nome, tb_dados_instituicao.enderecos, tb_dados_instituicao.nif, tb_dados_instituicao.telefone, tb_dados_instituicao.email, item_compras.*, c.*, f.nome fornecedor, a.designacao armazem, u.nome usuario , d.designacao documento , p.designacao produto

from  tb_dados_instituicao, item_compras
inner join tb_produto p on p.codigo = fk_produto
inner join compras c on c.pk_compra = fk_compra
inner join tb_fornecedor f on f.codigo = fk_fornecedor
inner join tb_armazem a on a.codigo = idArmazemFK
inner join tb_usuario u on u.codigo = codigo_usuario
inner join documento d on d.pk_documento = fk_documento
inner join ano_economico ae on ae.pk_ano_economico = fk_ano_economico


where
pk_compra =  $P{PK_COMPRA} 
/*
fk_fornecedor = 3
and 
idArmazemFK = 1
and
codigo_usuario = 15
and 
fk_documento = 9
*/]]>
	</queryString>
