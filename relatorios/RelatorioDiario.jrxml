	<queryString>
		<![CDATA[SELECT
     t.`codigo_produto` AS CODIGO_PRODUTO,
     p.`designacao` AS DESCRICAO_PRODUTO,
     SUM(t.quantidade)AS SOMA_QUANTIDADE,
     SUM(t.total)AS SOMA_TOTAL,
     tb_dados_instituicao.`nome` AS NOME_INSTITUICAO,
     tb_dados_instituicao.`telefone` AS TELEFONE,
     tb_dados_instituicao.`enderecos` AS ENDERECO,
     tb_dados_instituicao.`email` AS EMAIL,
     tb_dados_instituicao.`nif` AS NIF,
     v.`performance` AS v_performance,
     tb_stock.`preco_venda` AS PRECO_VENDA,
     p.`preco` AS PRECO_COMPRA,
     v.`status_eliminado` AS v_status_eliminado
FROM
     `tb_produto` p INNER JOIN `tb_stock` tb_stock ON p.`codigo` = tb_stock.`cod_produto_codigo`,
     `tb_item_venda` t,
     `tb_venda` v,
     `tb_dados_instituicao` tb_dados_instituicao

WHERE
     t.codigo_venda = v.codigo
 AND p.codigo = t.codigo_produto
AND v.`performance` = 'false'
AND v.`status_eliminado` = 'false'
 AND  v.dataVenda  BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}

 GROUP BY  t.codigo_produto]]>
	</queryString>
