	<parameter name="ID_ARMAZEM" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     tb_dados_instituicao.`nome` AS NOME_INSTITUICAO,
     tb_dados_instituicao.`telefone` AS TELEFONE,
     tb_dados_instituicao.`enderecos` AS ENDERECO,
     tb_dados_instituicao.`email` AS EMAIL,
     tb_dados_instituicao.`nif` AS NIF,
     tb_vasilhame.`descricao` AS tb_vasilhame_descricao,
     tb_vasilhame.`pk_vasilhame` AS tb_vasilhame_pk_vasilhame,
     tb_vasilhame.`qtd_existente` AS tb_vasilhame_qtd_existente,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_armazem.`codigo` AS tb_armazem_codigo,
     tb_armazem.`designacao` AS tb_armazem_designacao
FROM
     `tb_produto` tb_produto INNER JOIN `tb_vasilhame` tb_vasilhame ON tb_produto.`codigo` = tb_vasilhame.`fk_produto`
     INNER JOIN `tb_armazem` tb_armazem ON tb_vasilhame.`fk_armazem` = tb_armazem.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao

WHERE      tb_armazem.`codigo` = $P{ID_ARMAZEM}]]>
	</queryString>
