	<queryString>
		<![CDATA[SELECT
     SUM(tb_venda.`total_venda`)AS TOTAL_VENDA,
     tb_banco.`descrisao` AS tb_banco_descrisao,
     tb_venda.`idBanco` AS tb_venda_pk_banco

FROM
     `tb_banco` tb_banco INNER JOIN `tb_venda` tb_venda ON tb_banco.`idBanco` = tb_venda.`idBanco`

WHERE DATE(tb_venda.`dataVenda`) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2} 
 AND status_eliminado = 'false'


GROUP BY
     tb_venda.`idBanco`]]>
	</queryString>
