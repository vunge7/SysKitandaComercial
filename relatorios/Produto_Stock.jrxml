	<parameter name="ID_ARMAZEM" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_stock.`status` AS tb_stock_status,
     tb_fornecedor.`nome` AS tb_fornecedor_nome,
     tb_produto.`data_fabrico` AS tb_produto_data_fabrico,
     tb_produto.`data_expiracao` AS tb_produto_data_expiracao,
     tb_produto.`data_entrada` AS tb_produto_data_entrada,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_produto.`preco` AS preco,
     SUM(tb_stock.`quantidade_existente`*tb_produto.`preco`)AS TOTAL_PRECO_COMPRA,
     SUM(tb_stock.`quantidade_existente`*tb_stock.`preco_venda`)AS TOTAL_PRECO_VENDA,
     tb_stock.`preco_venda` AS tb_stock_preco_venda,
     tb_stock.`quantidade_existente` AS tb_stock_quantidade_existente,
     tb_armazem.`designacao` AS tb_armazem_designacao,
     tb_stock.`cod_armazem` AS tb_stock_cod_armazem,
     tb_tipo_produto.`designacao` AS tb_tipo_produto_designacao,
     tb_tipo_produto.`codigo` AS tb_tipo_produto_codigo
FROM
     `tb_produto` tb_produto INNER JOIN `tb_stock` tb_stock ON tb_produto.`codigo` = tb_stock.`cod_produto_codigo`
     INNER JOIN `tb_fornecedor` tb_fornecedor ON tb_produto.`cod_fornecedores` = tb_fornecedor.`codigo`
     INNER JOIN `tb_tipo_produto` tb_tipo_produto ON tb_produto.`cod_Tipo_Produto` = tb_tipo_produto.`codigo`
     INNER JOIN `tb_armazem` tb_armazem ON tb_stock.`cod_armazem` = tb_armazem.`codigo`
WHERE
     tb_stock.`cod_armazem` = $P{ID_ARMAZEM} AND  tb_tipo_produto.`codigo` <> 9
GROUP BY tb_produto.`codigo` AND tb_tipo_produto.`codigo`]]>
	</queryString>
