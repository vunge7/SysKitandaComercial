	<queryString>
		<![CDATA[SELECT
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_entrada.`data_entrada` AS tb_entrada_data_entrada,
     tb_entrada.`quantidade` AS tb_entrada_quantidade,
     tb_entrada.`idProduto` AS tb_entrada_idProduto
FROM
     `tb_produto` tb_produto INNER JOIN `tb_entrada` tb_entrada ON tb_produto.`codigo` = tb_entrada.`idProduto`
     INNER JOIN `tb_usuario` tb_usuario ON tb_entrada.`idUsuario` = tb_usuario.`codigo`
WHERE
      tb_entrada.`idProduto` = $P{ID_PRODUTO} AND tb_entrada.`data_entrada`  BETWEEN $P{DATA_INICIO} AND $P{DATA_FIM}]]>
