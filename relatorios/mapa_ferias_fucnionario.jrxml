	<queryString>
		<![CDATA[SELECT
     pedido_feria.`data_inicio` AS pedido_feria_data_inicio,
     pedido_feria.`data_fim` AS pedido_feria_data_fim,
     pedido_feria.`dias_ferias` AS pedido_feria_dias_ferias,
     pedido_feria.`data_registro` AS pedido_feria_data_registro,
     pedido_feria.`fk_funcionario` AS pedido_feria_fk_funcionario,
     pedido_feria.`pk_pedido_feria` AS pedido_feria_pk_pedido_feria,
     tb_funcionario.`idFuncionario` AS tb_funcionario_idFuncionario,
     tb_funcionario.`numero_funcionario` AS tb_funcionario_numero_funcionario,
     tb_funcionario.`nome` AS tb_funcionario_nome,
     tb_funcionario.`fk_empresa` AS tb_funcionario_fk_empresa,
     empresa.`pk_empresa` AS empresa_pk_empresa,
     empresa.`nome` AS empresa_nome,
     empresa.`telefone` AS empresa_telefone,
     empresa.`endereco` AS empresa_endereco,
     empresa.`NIF` AS empresa_NIF,
     empresa.`email` AS empresa_email,
     empresa.`logo_tipo` AS empresa_logo_tipo
FROM
     `tb_funcionario` tb_funcionario INNER JOIN `pedido_feria` pedido_feria ON tb_funcionario.`idFuncionario` = pedido_feria.`fk_funcionario`
     INNER JOIN `empresa` empresa ON tb_funcionario.`fk_empresa` = empresa.`pk_empresa`

WHERE DATE(pedido_feria.`data_registro`) BETWEEN $P{PARM_DATA_INICIO} AND $P{PARM_DATA_FIM}
AND tb_funcionario.`fk_empresa` = $P{PARM_ID_EMPRESA}

ORDER BY tb_funcionario.`nome` ASC]]>
	</queryString>
