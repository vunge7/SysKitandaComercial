	<queryString>
		<![CDATA[SELECT DISTINCT
     tb_venda.`codigo` AS tb_venda_codigo,
     tb_venda.`dataVenda` AS tb_venda_dataVenda,
     tb_venda.`total_venda` AS tb_venda_total_venda,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_venda.`total_geral` AS tb_venda_total_geral,
     tb_venda.`fk_documento` AS tb_venda_fk_documento
FROM
     `tb_venda` tb_venda INNER JOIN `tb_usuario` tb_usuario ON tb_venda.`codigo_usuario` = tb_usuario.`codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_usuario.`nome` = $P{NOME_USUARIO}
 AND tb_venda.`performance` = 'false'
 AND tb_venda.`status_eliminado` = 'false'
 AND tb_venda.`dataVenda` BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2}]]>
	</queryString>
