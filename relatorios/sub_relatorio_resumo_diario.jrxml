	<parameter name="PARM_DOCUMENTO" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     SUM(tb_venda.`total_venda`) AS TOTAL_VENDA,
     tb_usuario.`codigo` AS tb_usuario_codigo,
     tb_usuario.`nome` AS tb_usuario_nome,
     tb_venda.`codigo_usuario` AS tb_venda_codigo_usuario,
     tb_venda.`fk_documento` AS tb_venda_fk_documento,
     tb_venda.`idArmazemFK` AS tb_venda_idArmazemFK
FROM
     `tb_usuario` tb_usuario INNER JOIN `tb_venda` tb_venda ON tb_usuario.`codigo` = tb_venda.`codigo_usuario`
WHERE
     DATE(tb_venda.`dataVenda`) BETWEEN $P{PARM_DATA_1} AND $P{PARM_DATA_2}
 AND status_eliminado = 'false'
 AND tb_venda.`credito` = 'false'
 AND tb_venda.`idArmazemFK` = $P{SUB_PARM_CODIGO_ARMAZEM}
 AND tb_venda.`fk_documento` = $P{PARM_DOCUMENTO}
GROUP BY
     tb_venda.`codigo_usuario`]]>
	</queryString>
