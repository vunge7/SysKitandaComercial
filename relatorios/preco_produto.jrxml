	<queryString language="SQL">
		<![CDATA[SELECT
     tb_stock.`preco_venda` AS tb_stock_preco_venda,
     tb_produto.`designacao` AS tb_produto_designacao,
     tb_produto.`codigo` AS tb_produto_codigo,
     tb_produto.`codBarra` AS tb_produto_codBarra,
     tb_dados_instituicao.`nome` AS tb_dados_instituicao_nome,
     tb_stock.`preco_venda_grosso` AS tb_stock_preco_venda_grosso,
     tb_stock.`qtd_grosso` AS tb_stock_qtd_grosso
FROM
     `tb_produto` tb_produto INNER JOIN `tb_stock` tb_stock ON tb_produto.`codigo` = tb_stock.`cod_produto_codigo`,
     `tb_dados_instituicao` tb_dados_instituicao
WHERE
     tb_produto.`codBarra` = $P{BARRA}]]>
